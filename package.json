{
  "name": "panelkit-ui",
  "version": "1.0.2",
  "description": "Custom SCSS-based CSS + JS framework for admin dashboards",
  "main": "dist/main.css",
  "style": "dist/main.css",
  "exports": {
    ".": {
      "style": "./dist/main.css",
      "require": "./dist/panelkit.js",
      "import": "./dist/panelkit.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "shx mkdir -p dist",
    "build:css": "sass src/scss/main.scss dist/main.css --no-source-map",
    "build:css:min": "sass src/scss/main.scss dist/main.min.css --style=compressed --no-source-map",
    "build:js": "shx cp src/js/dropdown.js dist/panelkit.js",
    "build": "npm run prebuild && npm run build:css && npm run build:js",
    "build:min": "npm run build:css:min",
    "prepublishOnly": "npm run build && npm run build:min",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "css",
    "scss",
    "framework",
    "ui",
    "admin",
    "dashboard",
    "dropdown",
    "buttons"
  ],
  "author": "Joydip Paul <joydippaul72@gmail.com> (https://github.com/joydip-paul)",
  "license": "MIT",
  "devDependencies": {
    "sass": "^1.79.0",
    "shx": "^0.4.0"
  }
}
